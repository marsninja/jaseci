{
  "name": "all-in-one",
  "version": "1.0.0",
  "description": "Jac client application: all-in-one",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "build": "npm run compile && vite build --config .client-build/.jac-client.configs/vite.config.js",
    "dev": "vite dev --config .client-build/.jac-client.configs/vite.config.js",
    "preview": "vite preview --config .client-build/.jac-client.configs/vite.config.js",
    "compile": "babel compiled --out-dir build --extensions \".jsx,.js\" --out-file-extension .js"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.0",
    "antd": "^6.0.0"
  },
  "devDependencies": {
    "vite": "^6.4.1",
    "@babel/cli": "^7.28.3",
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "@vitejs/plugin-react": "^4.2.1",
    "typescript": "^5.3.3",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "modules": false
        }
      ],
      "@babel/preset-react"
    ]
  }
}